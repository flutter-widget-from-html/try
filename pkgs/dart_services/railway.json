{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "pkgs/dart_services/cloud_run.Dockerfile",
    "watchPatterns": [
      "/pkgs/dart_services/**"
    ]
  },
  "deploy": {
    "sleepApplication": true,
    "startCommand": "/bin/sh -c \"dart bin/server.dart --redis-url=$REDIS_URL --channel=stable\""
  }
}
